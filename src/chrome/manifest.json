{
"name": "YouTweak for YouTube\u2122 - Subscription Manager",
"version": "3.233",
"manifest_version": 2,

"options_page": "/optionspage/options.html",
"background": {
    "scripts": ["backgroundpage/eventpage.js"]
  },

"permissions": ["notifications", "storage","webNavigation", "*://youtube.com/*","*://*.youtube.com/*", "tabs", "*://mattie432.com/*",
"file://*/*"],

"browser_action": {
    "default_icon": "images/icon.png"
  },

"icons": { "16": "images/icon.png",
           "48": "images/icon.png",
          "128": "images/icon.png" },

"description": "Adds many useful tweaks to pages on the youtube site. Remove subscriptions, clear watched videos & more!",

"web_accessible_resources": ["images/icon.png", "contextscripts/videoplayer.js"],

"content_scripts": [
  {
    "matches": [
  "*://*.youtube.com/*",
  "*://*.youtube.com.br/*",
  "*://*.youtube.fr/*",
  "*://*.youtube.jp/*",
  "*://*.youtube.nl/*",
  "*://*.youtube.pl/*",
  "*://*.youtube.ie/*",
  "*://*.youtube.co.uk/*",
  "*://*.youtube.es/*",
  "*://*.youtube.it/*",
  "file://*/*"
  ],
    "js": ["backgroundpage/commonmethods.js", "contextscripts/versioncheck.js", "/contextscripts/everypage.js"]
  },{
      "matches": [
		"*://*.youtube.com/feed/*",
		"*://*.youtube.com.br/feed/*",
		"*://*.youtube.fr/feed/*",
		"*://*.youtube.jp/feed/*",
		"*://*.youtube.nl/feed/*",
		"*://*.youtube.pl/feed/*",
		"*://*.youtube.ie/feed/*",
		"*://*.youtube.co.uk/feed/*",
		"*://*.youtube.es/feed/*",
		"*://*.youtube.it/feed/*",
    "file://*/*"
	  ],
      "js": ["backgroundpage/commonmethods.js", "contextscripts/homepage_loadatend.js","contextscripts/loadvideoplayer.js", "contextscripts/videoplayer.js"],
    "run_at": "document_end"
    },{
      "matches": [

		"*://*.youtube.com/feed/*",
		"*://*.youtube.com.br/feed/*",
		"*://*.youtube.fr/feed/*",
		"*://*.youtube.jp/feed/*",
		"*://*.youtube.nl/feed/*",
		"*://*.youtube.pl/feed/*",
		"*://*.youtube.ie/feed/*",
		"*://*.youtube.co.uk/feed/*",
		"*://*.youtube.es/feed/*",
		"*://*.youtube.it/feed/*",
    "file://*/*"
	  ],
      "js": ["backgroundpage/commonmethods.js", "contextscripts/homepage_loadatstart.js"],
    "run_at": "document_start"
    },{
      "matches": [
		"*://*.youtube.com/watch*",
		"*://*.youtube.com.br/watch*",
		"*://*.youtube.fr/watch*",
		"*://*.youtube.jp/watch*",
		"*://*.youtube.nl/watch*",
		"*://*.youtube.pl/watch*",
		"*://*.youtube.ie/watch*",
		"*://*.youtube.co.uk/watch*",
		"*://*.youtube.es/watch*",
		"*://*.youtube.it/watch*"
	  ],
    "js": ["backgroundpage/commonmethods.js","contextscripts/loadvideoplayer.js", "contextscripts/videoplayer.js"],
    "run_at": "document_end"
    }
  ]
}
